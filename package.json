{
  "name": "react-smart-on-fhir-jsx-app",
  "version": "0.1.0",
  "description": "My First SMART on FHIR Application w/ React",
  "source": "src/index.html",
  "scripts": {
    "npm:install:global": "npm install --location=global yarn serve open-cli",
    "setup": "npm run npm:install:global",
    "standard": "standard --fix src/**/*.jsx",
    "lint:js": "npm run standard",
    "lint": "npm run lint:js",
    "preparcel": "npm run lint",
    "parcel": "parcel",
    "parcel:watch": "npm run parcel -- watch",
    "parcel:build": "npm run parcel -- build",
    "test": "exit 0",
    "dev": "npm run parcel:watch",
    "dist": "npm run parcel:build",
    "serve:dist": "serve ./dist -p 3000",
    "prestart": "npm run dist",
    "start": "npm run serve:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:percebus/react-smart-on-fhir-jsx-app.git"
  },
  "keywords": [
    "hello-world",
    "parcel",
    "react",
    "html",
    "web"
  ],
  "author": "percebus",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/percebus/react-smart-on-fhir-jsx-app/issues"
  },
  "homepage": "https://github.com/percebus/react-smart-on-fhir-jsx-app",
  "dependencies": {
    "fhir-visualizers": "^0.0.6",
    "fhirclient": "^2.4.0",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "parcel": "latest",
    "process": "^0.11.10",
    "standard": "latest"
  }
}
